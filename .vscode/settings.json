{
    "python.languageServer": "Pylance",
    "python.analysis.typeCheckingMode": "off", // or "strict" if you want most checks
    "python.analysis.diagnosticSeverityOverrides": {
        "reportOptionalMemberAccess": "none",
        "reportOptionalSubscript": "none",
        "reportReturnType": "none",
        "reportGeneralTypeIssues": "none",
        "reportMissingModuleSource": "none"
    },
    "python.terminal.activateEnvironment": false,
    "micropico.openOnStart": true,
    "python.analysis.typeshedPaths": [
        "~/.micropico-stubs/included"
    ],
    "python.analysis.extraPaths": [
        "~/.micropico-stubs/included"
    ],
    "micropico.syncFolder": "",
    "micropico.syncFileTypes": [
        "py",
        "txt",
    ],
    "cursorpyright.analysis.diagnosticSeverityOverrides": {
        "reportMissingModuleSource": "none"
    },
    "cursorpyright.analysis.extraPaths": [
        "~/.micropico-stubs/included"
    ],
    "cursorpyright.analysis.typeCheckingMode": "basic",
    "cursorpyright.analysis.typeshedPaths": [
        "~/.micropico-stubs/included"
    ],
}